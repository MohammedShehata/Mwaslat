<body onload="editNode()">
	<div id="map_canvas" style="width:100%; height:100%"></div>
	<div class="container">
		<div class="row">
			<div class="span9">
				<%if(!notice.nil?)%>
					<br/>
					<div class="alert alert-info">
					    <button class="close" data-dismiss="alert">Ã—</button>
					    <%= notice %>
				    </div>
			    <%end%>
				<%= form_for @node  , :html => { :class => 'form-horizontal' }  do |f| %> <h2 >Update Place</h2>
				<hr/>
				<%= render 'node_fields',:f => f, :show_category => true, :editable => true %>
				<div class="form-actions">
					<%= f.submit('Save', :onclick => "fillNodePath();",:class =>'btn btn-small btn-primary') %>
					<%= link_to 'Cancel', nodes_path, :class => 'btn' %>
				</div>
				<% end %>
			</div>
		</div>
	</div>
</body>